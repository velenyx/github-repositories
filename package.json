{
  "name": "github-repos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:ts:check": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "prettier:check": "prettier --check \"**/*.{js,ts,tsx,css,mdx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx,css,mdx}\"",
    "generate-gql": "graphql-codegen -r dotenv/config --config ./app/_src/graphql/codegen.yml && prettier --write ./app/_src/graphql/__generated__.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@mantine/core": "^7.3.2",
    "@mantine/hooks": "^7.3.2",
    "@tabler/icons-react": "^2.44.0",
    "graphql": "^16.8.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "postcss-preset-mantine": "^1.11.1",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.1",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.0.2"
}
